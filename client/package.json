    "link": "^0.1.5",
    "react": "15.4.2",
    "react-native": "0.42.0",
    "react-native-camera-roll-picker": "^1.1.9",
    "react-native-fetch-blob": "^0.10.2",
    "react-native-image-picker": "^0.26.2",
    "react-native-modal-dropdown": "^0.4.2",
    "react-native-permissions": "^0.2.7",
    "react-native-router-flux": "^3.38.0",
    "react-native-simple-radio-button": "^2.5.0-alpha.1",
    "react-native-snap-carousel": "^2.1.0",
